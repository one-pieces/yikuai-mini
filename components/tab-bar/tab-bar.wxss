/* components/tab-bar/tab-bar.wxss */
.op-tab-bar {
  position: absolute;
  left: 0;
  //width: 100%;
  display: flex;
  justify-content: space-between;
  padding: 0 100rpx;
  box-sizing: border-box;
  font-size: 20rpx;
}

.tab-item {
  padding: 20rpx 50rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.tab-item image {
  width: 45rpx;
  height: 45rpx;
}